{
  "name": "angular-meetup",
  "version": "0.1.5",
  "description": "Angular component to authentication with oauth2 using meetup login.",
  "homepage": "https://github.com/charlierlee/angular-meetup",
  "main": "./dist/angular-meetup.js",
  "scripts": {
    "test": "grunt test",
    "start": "node dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/charlierlee/angular-meetup.git"
  },
  "keywords": [
    "AngularJS",
    "meetup",
    "oauth",
    "authentication",
    "authorization"
  ],
  "author": "charlierlee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charlierlee/angular-meetup/issues"
  },
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    "babel-core": "^6.26.3",
    "gulp": "^4.0.0",
    "gulp-angular-templatecache": "^2.2.1",
    "gulp-babel": "^7.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-debug": "^4.0.0",
    "gulp-filter": "^5.1.0",
    "gulp-flatten": "^0.4.0",
    "gulp-minify-css": "^1.2.4",
    "gulp-rename": "^1.3.0",
    "gulp-static-hash": "^0.1.4",
    "gulp-uglify": "^3.0.0",
    "gulp-util": "^3.0.8",
    "npmfiles": "^0.1.1",
    "pump": "^3.0.0"
